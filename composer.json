{
  "name": "astrotech/rabbitmq",
  "require": {
    "php": "^8.1",
    "ext-amqp": "*",
    "php-amqplib/php-amqplib": "^3.5",
    "vlucas/phpdotenv": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "process-timeout": 0
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
